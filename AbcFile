pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Develop') {
            steps {
                echo 'Developing Plan'
            }
        }
         stage('Build') {
            steps {
                echo 'Building'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
